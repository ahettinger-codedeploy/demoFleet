	var blnConfirm;
	blnConfirm = confirm('Are you sure you want to close this RFP?');
	if (blnConfirm)
	{
		top.blank.location.href = "/eduENGINE/CloseRFP.cfm?code=" + feeCode + "&dataSource=" + dataSource + "&site=" + site;
