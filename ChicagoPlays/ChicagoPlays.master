    	favstyle+= 'PRICE: <strong style="color:#A4A5A0;">'+price+'</strong></div></div>';
    }
	else if(price != '')
	{
	     favstyle+='<div  style="clear:both;=margin-bottom:-20px"><div class="left price_box" style="padding-left:14px;font-size:12px;font-weight:bold;text-transform:uppercase;color:#6FF3FF;">';

    	favstyle+= 'PRICE: <strong style="color:#A4A5A0;">'+price+'</strong></div></div>';
	}
